<%= form_with(model: note) do |form| %>
  <div class="">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="">
    <%= form.label :content %>
    <%= form.rich_text_area :content %>
  </div>


<div class="">
  <%= form.hidden_field :user_id, value: current_user.id %>
</div>

  <div class="">
    <%= form.submit %>
  </div>

<% end %>



